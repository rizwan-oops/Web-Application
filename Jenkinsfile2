#!/usr/bin/env groovy

@Library('shared-libraryL@main') _ //master or whatever branch

pipeline{

      agent any
        
        stages{   
		 stage('maven build'){
                  steps{
                     echo 'Success from file '
		       }  
                 }
		
               stage ('Check logs') {
                    steps {
                        filterLogs ('Commit', 50)
                    }
                }
		
		
               stage ('Calculator') {
                    steps {
                       echo 'Welcome from Calcilator Stage'
			    script{
				    calculator.add(10,12)
				    calculator.mult(11,12)
				    calculator.buildn()
			    }
                    }
                }
		
           }	       	     	         
}
