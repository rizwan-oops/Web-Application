#!/usr/bin/env groovy

@Library('shared-libraryL@main') _ //master or whatever branch

pipeline{

      agent any
        
        stages{   
		 stage('maven build'){
                  steps{
                      script{	    	               
		    	                sh "mvn -f $WORKSPACE/pom.xml clean install"
                      	  }
		       }  
                 }
		
               stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 1)
                    }
                }
		
           }	       	     	         
}
