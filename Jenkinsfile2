#!/usr/bin/env groovy



pipeline{

      agent any
        
        stages{

              stage('maven build'){
                  steps{
                      script{
		    	                sh "mvn -f $WORKSPACE/pom.xml clean install"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 50)
                    }
                }
		
           }	       	     	         
}